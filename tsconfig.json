{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "experimentalDecorators": false,
    "resolveJsonModule": true,
    "strict": false,
    "plugins": [
      {
        "name": "typescript-deno-plugin",
        "enable": true, // default is `true`
        "importmap": "import_map.json"
      }
    ]
  }
}
